{{#each docked as |id index|}}
  {{docked-composer id=id
                    docked=docked
                    maxIndex=maxIndex
                    index=index
                    removeDocked="removeDocked"
                    updateId="updateId"
                    targetUsernames=targetUsernames}}
{{/each}}
{{#if showQuickButton}}
  {{d-button class='quick-button' action="openQuick" icon=siteSettings.quick_message_quick_button_icon}}
{{/if}}
